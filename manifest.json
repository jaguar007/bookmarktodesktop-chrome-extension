{
  "name": "Bookmark to Desktop",
  "version": "1.0",
  "description": "Chrome extension for the Bookmark to desktop Android application",
  "options_page": "options.html",
  "background_page": "background.html",
  "icons": { "128":"icon.png" },
  "browser_action": {
    "default_icon":"icon.png",
    "popup":"popup.html"
  },
  "permissions": [
    "bookmarks",
    "notifications",
    "tabs",
    "https://bookmarktodesktop.appspot.com/"
  ]
}
